{
    "compilerOptions": {
        "declaration": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitUseStrict": true,
        "outDir": "../es5",
        "removeComments": true,
        "sourceMap": true,
        "target": "es5"
    },
    "exclude": [
        "es5",
        "nbproject",
        "node_modules",
        "public"
    ]
}